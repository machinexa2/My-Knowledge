		git clone https://github.com/vortexau/dnsvalidator.git && cd dnsvalidator && python3 setup.py install && cd ..
	else
		cd dnsvalidator && gitfp && python3 setup.py install && cd ..
	fi
	if [[ ! -d Interlace ]]; then
		git clone https://github.com/codingo/Interlace && cd Interlace && python3 setup.py install && cd ..
	else
		cd Interlace && gitfp && python3 setup.py install && cd ..
	fi
	if [[ ! -d massdns ]]; then
		git clone https://github.com/blechschmidt/massdns.git && cd massdns && make && make install && cd ..
