		git clone https://github.com/devanshbatham/FavFreak && addbinary favfreak.py favfreak && cd ..
	else
		cd FavFreak && gitfp && addbinary favfreak.py favfreak && cd ..
	fi
	if [[ ! -d squidclient ]]; then
		git clone https://github.com/limitedeternity/squidclient
	else
		cd squidclient && cargo build --release && (cd target/release && addbinary squidclient squidclient) && cd ..
cd $CURRENT
cd /var/www/html
